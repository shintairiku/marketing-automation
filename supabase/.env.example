# supabase/.env

# --- 必須 ---
POSTGRES_PASSWORD=your_secure_password # 以前設定したDBパスワード
POSTGRES_USER=supabase # Supabaseのデフォルトユーザー
POSTGRES_DB=postgres # SupabaseのデフォルトDB名
JWT_SECRET=your_super_secret_jwt_secret_with_at_least_32_characters # 32文字以上のJWTシークレット

# --- Supabase CLI (supabase start) が生成するキー (初回にコピー) ---
ANON_KEY=your_anon_key # supabase start で表示されたキー
SERVICE_ROLE_KEY=your_service_role_key # supabase start で表示されたキー

# --- 以下は通常デフォルト値でOK ---
POSTGRES_HOST=db
KONG_URL=http://kong:8000
GOTRUE_URL=http://gotrue:9999
REALTIME_URL=http://realtime:4000/socket
STORAGE_URL=http://storage:5000
FUNCTIONS_URL=http://functions:9000
STUDIO_PORT=54323
# ... その他 supabase start で表示されるキー